    "@nrwl/eslint-plugin-nx": "13.8.0",
    "@nrwl/jest": "13.8.0",
    "@nrwl/js": "13.8.0",
    "@nrwl/linter": "13.8.0",
    "@nrwl/next": "^13.8.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "13.8.0",
    "@nrwl/tao": "13.8.0",
