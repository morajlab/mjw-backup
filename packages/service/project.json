{
  "root": "packages/service",
  "sourceRoot": "packages/service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/service",
        "main": "packages/service/src/index.ts",
        "tsConfig": "packages/service/tsconfig.app.json",
        "assets": ["packages/service/*.md"]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "service:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/service"],
      "options": {
        "jestConfig": "packages/service/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
